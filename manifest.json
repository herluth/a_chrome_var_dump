{
  "manifest_version": 2,

  "name": "Var_Dump Syntax Highlighter",
  "description": "This extension adds syntax highlighting to var_dumps",
  "version": "1.0",

  "permissions": [
    ""
  ],
  
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.*.*/*"],
      "css": ["mystyle.css"],
      "js": ["myscript.js"],
      "run_at" : "document_idle"
    }
  ]
}
